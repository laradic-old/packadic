{
    "name": "laradic/packadic",
    "type": "laradic-extension",
    "description": "Packadic theme for laradic themes/extensions",
    "homepage": "https://github.com/laradic/packadic",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl",
            "role": "Package Author/Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "laradic/support": "1.2.*",
        "laradic/config": "1.3.*",
        "laradic/extensions": "0.1.*",
        "laradic/themes": "0.1.*"
    },
    "autoload": {
        "psr-4": {
            "Laradic\\Packadic\\": "src/"
        }
    }
}
